{
  "name": "guida",
  "version": "0.1.0",
  "description": "Guida is a functional programming language that builds upon the solid foundation of Elm, offering backward compatibility with all existing Elm 0.19.1 projects",
  "author": "Decio Ferreira",
  "license": "BSD-3-Clause",
  "main": "./lib/guida.js",
  "bin": {
    "guida": "bin/index.js"
  },
  "scripts": {
    "build": "./lib/guida.sh src/Terminal/Main.elm",
    "test": "npm-run-all --parallel test:*",
    "test:jest": "jest",
    "test:elm": "elm-test tests/Parse",
    "test:elm-review": "elm-review --ignore-dirs tests/assets",
    "test:elm-format-validate": "elm-format . --validate",
    "elm-format": "elm-format . --yes"
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "form-data": "^4.0.2",
    "mock-xmlhttprequest": "^8.4.1",
    "tmp": "^0.2.3",
    "which": "^5.0.0"
  },
  "devDependencies": {
    "elm": "^0.19.1-6",
    "elm-format": "^0.8.7",
    "elm-review": "^2.13.2",
    "elm-test": "^0.19.1-revision15",
    "guida": "^0.3.0-alpha",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "uglify-js": "^3.19.3"
  }
}